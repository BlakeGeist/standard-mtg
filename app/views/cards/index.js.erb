//shadows over instarad
if ($setId == 1) {
  $('[data-set-id=' + $setId + '] .cards-container').append('<%= j render 'soi_cards' %>');

  <% if @soi_cards.next_page %>
    $('[data-set-id=' + $setId + '] .pagination').replaceWith('<%= j will_paginate(@soi_cards) %>');
  <% else %>
    $('[data-set-id=' + $setId + '] .pagination').remove();
  <% end %>
}
