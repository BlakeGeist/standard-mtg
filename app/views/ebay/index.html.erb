<ul class="auctions">

  <% @GetOrders["item_array"]["item"].each do |item| %>

    <li>

      <ul class="auctions-auction">

        <% item.each do |k, v| %>

          <li class="auctions-auction-item">

            <%= render 'loop_hash', :v => v, :k => k %>

          </li>

        <% end %>

      </ul>

    </li>

  <% end %>

</ul>
