<ul>

  <% @GetOrders["item_array"]["item"].each do |item| %>

    <li>

      <ul>

        <% item.each do |k, v| %>

          <li>

            <%= k %>:

            <% if v.is_a? Hash %>

              <ul>

                <% v.each do |k, v| %>

                  <li><%= "#{k}:#{v}" %></li>

                <% end %>

              </ul>

            <% else %>

              <%= v %>

            <% end %>

          </li>

        <% end %>

      </ul>

    </li>

  <% end %>

</ul>
